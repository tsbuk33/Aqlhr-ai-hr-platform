{
  "name": "aqlhr-backend-5layer",
  "version": "1.0.0",
  "description": "AQLHR 5-Layer System Architecture Backend",
  "main": "layer2-ai-orchestration/main.py",
  "scripts": {
    "start": "python layer2-ai-orchestration/main.py",
    "dev": "python layer2-ai-orchestration/main.py",
    "deploy": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh deploy",
    "stop": "./scripts/deploy.sh stop",
    "restart": "./scripts/deploy.sh restart",
    "health": "./scripts/deploy.sh health",
    "logs": "./scripts/deploy.sh logs",
    "build": "./scripts/deploy.sh build",
    "cleanup": "./scripts/deploy.sh cleanup",
    "test": "python -m pytest tests/",
    "test:integration": "python -m pytest tests/integration/",
    "test:load": "k6 run tests/load/api_load_test.js",
    "lint": "flake8 layer2-ai-orchestration/ layer3-business-logic/ layer4-integration/",
    "format": "black layer2-ai-orchestration/ layer3-business-logic/ layer4-integration/",
    "security": "bandit -r layer2-ai-orchestration/ layer3-business-logic/ layer4-integration/",
    "docs": "sphinx-build -b html docs/ docs/_build/",
    "migrate": "alembic upgrade head",
    "seed": "python scripts/seed_data.py",
    "backup": "python scripts/backup.py",
    "restore": "python scripts/restore.py"
  },
  "keywords": [
    "hr",
    "ai",
    "microservices",
    "saudi-arabia",
    "government-integration",
    "autonomous-ai",
    "vision-2030"
  ],
  "author": "AQLHR Team",
  "license": "MIT",
  "dependencies": {
    "fastapi": "^0.104.1",
    "uvicorn": "^0.24.0",
    "pydantic": "^2.5.0",
    "psycopg2-binary": "^2.9.9",
    "redis": "^5.0.1",
    "sqlalchemy": "^2.0.23",
    "alembic": "^1.13.1",
    "aiohttp": "^3.9.1",
    "openai": "^1.3.7",
    "transformers": "^4.36.0",
    "torch": "^2.1.1",
    "numpy": "^1.24.3",
    "pandas": "^2.0.3",
    "scikit-learn": "^1.3.2",
    "python-multipart": "^0.0.6",
    "python-dateutil": "^2.8.2",
    "python-jose": "^3.3.0",
    "passlib": "^1.7.4",
    "bcrypt": "^4.1.2",
    "celery": "^5.3.4",
    "flower": "^2.0.1",
    "prometheus-client": "^0.19.0",
    "structlog": "^23.2.0",
    "sentry-sdk": "^1.38.0"
  },
  "devDependencies": {
    "pytest": "^7.4.3",
    "pytest-asyncio": "^0.21.1",
    "pytest-cov": "^4.1.0",
    "black": "^23.11.0",
    "flake8": "^6.1.0",
    "mypy": "^1.7.1",
    "bandit": "^1.7.5",
    "sphinx": "^7.2.6",
    "sphinx-rtd-theme": "^1.3.0"
  },
  "engines": {
    "python": ">=3.11.0",
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsbuk33/Aqlhr-ai-hr-platform.git"
  },
  "bugs": {
    "url": "https://github.com/tsbuk33/Aqlhr-ai-hr-platform/issues"
  },
  "homepage": "https://github.com/tsbuk33/Aqlhr-ai-hr-platform#readme"
}

